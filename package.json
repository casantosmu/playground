{
  "name": "postgresql-benchmarks",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.11.0"
  },
  "imports": {
    "#lib/*": "./lib/*.js",
    "#dataset.json": "./dataset.json"
  },
  "scripts": {
    "benchmark:all": "npm run benchmark:left-join",
    "benchmark:left-join": "node --env-file=.env benchmark/left-join.js",
    "db:all": "npm run db:create-tables && npm run db:seed",
    "db:create-tables": "node --env-file=.env scripts/create-tables.js",
    "db:seed": "node --env-file=.env scripts/seed.js",
    "codegen:dataset": "node scripts/generate-dataset.js"
  },
  "dependencies": {
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "autocannon": "^7.14.0"
  }
}
